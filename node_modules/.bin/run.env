#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../run.env/bin/run.env" "$@"
  ret=$?
else 
  node  "$basedir/../run.env/bin/run.env" "$@"
  ret=$?
fi
exit $ret
