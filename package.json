{
  "name": "telescope",
  "version": "0.0.1",
  "description": "A tool for tracking blogs in orbit around Seneca's open source involvement",
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "automock": false,
    "setupFiles": [
      "./test/setupJest.js"
    ]
  },
  "scripts": {
    "eslint": "eslint --ignore-path .gitignore .",
    "eslint-fix": "eslint --fix --ignore-path .gitignore .",
    "stylelint": "stylelint src/**/*.css",
    "lint": "npm run eslint",
    "postlint": "npm run stylelint",
    "prettier": "prettier --write \"./**/*.{md,json,html,css,js,yml}\"",
    "pretest": "npm run lint",
    "test": "npm run jest",
    "jest": "cross-env MOCK_REDIS=1 jest",
    "start": "node src",
    "debug-server": "nodemon server.js",
    "webhint": "hint http://localhost:3000",
    "test-hint": "hint ./test/test_files"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": "Seneca-CDOT/telescope",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Seneca-CDOT/telescope/issues"
  },
  "homepage": "https://github.com/Seneca-CDOT/telescope#readme",
  "dependencies": {
    "bull": "^3.11.0",
    "bull-board": "^0.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "feedparser-promised": "^2.0.1",
    "get-urls": "^9.2.0",
    "ioredis": "^4.14.1",
    "ioredis-mock": "^4.18.2",
    "jest-fetch-mock": "^2.1.2",
    "jsdom": "^15.2.1",
    "node-fetch": "^2.6.0",
    "node-summarizer": "^1.0.7",
    "nodemailer": "^6.3.1",
    "parse-github-url": "^1.0.2",
    "passport": "^0.4.0",
    "passport-openid": "^0.4.0",
    "pino": "^5.13.6",
    "pino-pretty": "^3.3.0",
    "reading-level": "0.0.7",
    "sentiment": "^5.0.2",
    "spam-filter": "^1.1.1",
    "valid-url": "^1.0.9",
    "wordcount": "^1.1.1"
  },
  "devDependencies": {
    "@hint/configuration-all": "^1.0.13",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "hint": "^5.4.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
